<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.fsu.cs.mysql_test">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Register" />
        <activity android:name=".Request" />
        <activity android:name=".Information"/>
        <activity android:name=".InviteList"/>
        <activity android:name=".ListMatch"/>
        <activity android:name=".EventsList"/>
        <activity android:name=".FriendRequest"/>
        <activity android:name=".FriendNotifications" />
        <activity android:name=".EventNotifList"/>
        <activity android:name=".ForgotPassword"/>

        <activity
            android:name=".Tabs"
            android:label="@string/title_activity_tabs"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>